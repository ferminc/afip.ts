{
  "name": "afip.ts",
  "version": "3.2.3",
  "description": "Afip typescript SDK",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "postbuild": "rimraf lib/soap/wsdl && copyfiles -u 2 src/soap/wsdl/* lib/soap/",
    "prepare": "npm run build",
    "test": "jest",
    "doc": "typedoc --theme oxide --name 'afip.ts' --out docs src/index.ts",
    "wsdl-gen": "wsdl-to-ts --",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "author": "valiuLab",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^29.5.1",
    "@types/node-forge": "^1.3.11",
    "@types/underscore": "^1.11.5",
    "@types/xml2js": "^0.4.14",
    "copyfiles": "^2.4.1",
    "jest": "^28.1.3",
    "rimraf": "^5.0.10",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitepress": "1.2.3",
    "wsdl-to-ts": "^0.2.3",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
    "winston": "^3.10.0",
    "wsdl-tsclient": "^1.7.1"
  },
  "dependencies": {
    "node-forge": "^1.3.2",
    "soap": "^1.6.0",
    "xml2js": "^0.6.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferminc/afip.ts.git"
  },
  "bugs": {
    "url": "https://github.com/ferminc/afip.ts/issues"
  },
  "homepage": "https://github.com/ferminc/afip.ts#readme",
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "web-service",
    "factura-electronica",
    "padron",
    "afip",
    "sdk",
    "package",
    "api",
    "soap",
    "rest",
    "endpoint",
    "wsaa"
  ]
}
